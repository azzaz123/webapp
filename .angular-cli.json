{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "webapp"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "manifest.json",
        "service-worker.js",
        "doubleclick",
        "eyeblaster",
        "flashtalking",
        "ftlocal"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "tsl",
      "styles": [
        "../node_modules/ngx-toastr/toastr.css",
        "../node_modules/ng2-dnd/style.css",
        "style/styles.scss"
      ],
      "scripts": [
        "../node_modules/pouchdb/dist/pouchdb.js",
        "../node_modules/stanza.io/build/stanzaio.bundle.js",
        "../node_modules/visibilityjs/lib/visibility.core.js",
        "../node_modules/appboy-web-sdk/appboy.min.js",
        "./assets/js/adsConfig.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "docker": "environments/environment.docker.ts",
        "beta": "environments/environment.beta.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "component": {
    },
    "build": {
      "preserveSymlinks": true
    }
  }
}
