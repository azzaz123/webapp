{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "webapp": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "preserveSymlinks": true,
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/service-worker.js",
              "src/doubleclick",
              "src/eyeblaster",
              "src/flashtalking",
              "src/ftlocal"
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/@beyerleinf/ngx-dnd/styles.css",
              "src/style/styles.scss"
            ],
            "scripts": [
              "node_modules/pouchdb/dist/pouchdb.js",
              "node_modules/stanza.io/build/stanzaio.bundle.js",
              "node_modules/visibilityjs/lib/visibility.core.js",
              "node_modules/appboy-web-sdk/appboy.min.js",
              "node_modules/indexeddb-getall-shim/IndexedDB-getAll-shim.js",
              "src/assets/js/adsConfig.js",
              "src/assets/js/quantcast.js"
            ]
          },
          "configurations": {
            "docker": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.docker.ts"
                }
              ]
            },
            "beta": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.beta.ts"
                }
              ]
            },
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            },
            "es": {
              "i18nFile": "src/locale/es.xtb",
              "i18nFormat": "xtb",
              "i18nLocale": "es"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "webapp:build"
          },
          "configurations": {
            "docker": {
              "browserTarget": "webapp:build:docker"
            },
            "beta": {
              "browserTarget": "webapp:build:beta"
            },
            "production": {
              "browserTarget": "webapp:build:production"
            },
            "es": {
              "browserTarget": "webapp:build:es"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "webapp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/pouchdb/dist/pouchdb.js",
              "node_modules/stanza.io/build/stanzaio.bundle.js",
              "node_modules/visibilityjs/lib/visibility.core.js",
              "node_modules/appboy-web-sdk/appboy.min.js",
              "src/assets/js/adsConfig.js",
              "src/assets/js/quantcast.js"
            ],
            "styles": [
              "node_modules/ngx-toastr/toastr.css",
              "node_modules/@beyerleinf/ngx-dnd/styles.css",
              "src/style/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/manifest.json",
              "src/service-worker.js",
              "src/doubleclick",
              "src/eyeblaster",
              "src/flashtalking",
              "src/ftlocal"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "webapp-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "webapp:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "e2e/tsconfig.e2e.json"
            ],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "webapp",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "tsl",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "tsl"
    }
  }
}
