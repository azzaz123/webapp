{
  "compilerOptions": {
    "target": "es6",
    "baseUrl": "./src",
    "paths": {
      "@core/*": [
        "app/core/*"
      ],
      "@shared/*": [
        "app/shared/*"
      ],
      "@api/*": [
        "app/api/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@environments/*": [
        "environments/*"
      ],
      "@features/*": [
        "app/features/*"
      ],
      "@fixtures/*": [
        "tests/*"
      ],
      "@layout/*": [
        "app/layout/*"
      ],
      "@public/*": [
        "app/public/*"
      ],
      "@private/*": [
        "app/private/*"
      ],
      "@storybook-config/*": [
        "../.storybook/*"
      ],
      "@stories/*": [
        "stories/*"
      ],
      "@data/user": [
        "app/data/user/index.ts"
      ],
      "@tests/shared": [
        "tests/shared/index.ts"
      ],
      "@configs/*": [
        "configs/*"
      ],
      "@wallapop-web-components/*": [
        "scripts/wallapop-web-components/*"
      ]
    },
    "outDir": "./dist/out-tsc",
    "module": "es2020",
    "moduleResolution": "node",
    "sourceMap": true,
    "importHelpers": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "angularCompilerOptions": {
    "strictInputTypes": true,
    "strictInputAccessModifiers": true,
    "strictNullInputTypes": true,
    "strictAttributeTypes": true,
    "strictSafeNavigationTypes": true,
    "strictDomLocalRefTypes": true,
    "strictOutputEventTypes": true,
    "strictDomEventTypes": true,
    "strictContextGenerics": true,
    "strictLiteralTypes": true
  }
}
