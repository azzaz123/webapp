{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "builddev": "ng build --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker": "ng build --configuration=docker --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker-en": "ng build --configuration=docker --aot --no-progress",
    "buildbeta": "ng build --prod --configuration=beta --output-hashing all --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildbeta-en": "ng build --configuration=beta --output-hashing all --aot --no-progress",
    "buildprod": "ng build --prod --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildprod-en": "ng build --prod --aot --no-progress",
    "i18n": "ng xi18n --i18n-format=xmb --output-path=locale",
    "serve": "ng serve --aot --configuration=es",
    "test": "ng test --watch=false --no-progress --browsers HeadlessChrome",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "svgo": "svgo -f ./src/assets/icons --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}; svgo -f ./src/assets/images --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}"
  },
  "private": true,
  "dependencies": {
