{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "node scripts/postinstall/remove-unused-moment-locales.js && ngcc",
    "start": "./scripts/docknum/start.sh",
    "start-beta": "ng serve -c=beta",
    "builddev": "ng build --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker": "ng build -c=docker --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker-en": "ng build -c=docker --no-progress",
    "buildbeta": "ng build -c=beta --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "buildbeta-en": "ng build -c=beta --no-progress",
    "buildprod": "ng build --prod --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildprod-en": "ng build --prod --no-progress",
    "i18n": "ng xi18n --i18n-format=xmb --output-path=locale",
    "serve": "ng serve -c=es",
    "test-once": "jest -c=src/configs/jest/jest.config.js",
    "test": "yarn test-once --watch",
    "test-ci": "yarn test-once --ci",
    "test-coverage": "yarn test-once --coverage && ./scripts/tests/open-coverage.sh",
    "test-clean": "rm -rf ./coverage && yarn test-once --clearCache",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "svgo": "svgo -f ./src/assets/icons --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}; svgo -f ./src/assets/images --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}",
    "mparticle": "yarn install && yarn upgrade mparticle_json_validation && node scripts/mparticle.js",
    "analyze-bundle": "yarn buildprod --statsJson=true && yarn webpack-bundle-analyzer dist/stats-es2015.json",
    "libphonenumber-metadata": "./scripts/generate-metadata/generate-metadata.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.2",
    "@angular/common": "^9.1.2",
    "@angular/compiler": "^9.1.2",
    "@angular/core": "^9.1.2",
    "@angular/forms": "^9.1.2",
    "@angular/localize": "~9.1.1",
    "@angular/platform-browser": "^9.1.2",
    "@angular/platform-browser-dynamic": "^9.1.2",
    "@angular/router": "^9.1.2",
    "@mparticle/web-appboy-kit": "^2.0.4",
    "@mparticle/web-sdk": "^2.9.9-rc.1",
    "@ng-bootstrap/ng-bootstrap": "6.0.3",
    "bootstrap": "4.0.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.7.0",
    "libphonenumber-js": "^1.6.8",
    "lodash-es": "^4.17.15",
    "moment": "^2.26.0",
    "ng2-dnd": "^5.0.2",
    "ngx-autosize": "1.6.4",
    "ngx-cookie": "4.0.2",
    "ngx-device-detector": "1.3.11",
    "ngx-echarts": "^4.2.2",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-permissions": "^7.0.3",
    "rxjs": "^6.5.5",
    "spain-id": "^1.0.9",
    "stanza.io": "https://github.com/Wallapop/stanza.io#v8.0.5",
    "tslib": "^1.10.0",
    "uuid": "^8.3.1",
    "visibilityjs": "^2.0.1",
    "web-animations-js": "^2.3.2",
    "web-styleguide": "git+ssh://git@github.com:Wallapop/web-styleguide#^0.7.1",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "9.1.1",
    "@angular/compiler-cli": "^9.1.2",
    "@angular/language-service": "^9.1.2",
    "@types/echarts": "^4.6.0",
    "@types/heremaps": "3.0.12",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "@types/uuid": "^8.3.0",
    "canvas": "^2.6.1",
    "codelyzer": "^5.1.2",
    "glob": "^7.1.4",
    "husky": "^4.3.0",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.1.3",
    "mparticle_json_validation": "git+ssh://git@github.com:Wallapop/mparticle_json_validation#0.41.0",
    "prettier": "2.1.2",
    "pretty-quick": "^3.1.0",
    "retry": "^0.12.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.8.3",
    "webpack-bundle-analyzer": "^3.6.0",
    "json-schema-to-typescript": "^7.0.0"
  }
}
