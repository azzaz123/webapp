{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "./scripts/postinstall/postinstall.sh",
    "start": "./scripts/docknum/start.sh",
    "start-beta": "ng serve -c=beta",
    "start-italian": "yarn start beta -c=it",
    "builddev": "ng build --no-progress",
    "builddocker": "ng build -c=docker --no-progress",
    "buildbeta": "ng build -c=beta --no-progress",
    "buildprod": "ng build -c=production --no-progress",
    "reinstall": "rm -rf node_modules && yarn install",
    "i18n": "yarn generate-angular-messages && yarn i18n-helper:merge-phrase",
    "generate-angular-messages": "ng extract-i18n --format=json --output-path=src/locale --out-file=messages.en.json",
    "serve": "ng serve -c=es",
    "test-once": "jest -c=src/configs/jest/jest.config.js",
    "test": "yarn test-once --watch",
    "test-ci": "yarn test-once --ci",
    "test-coverage": "yarn test-once --coverage && ./scripts/tests/open-coverage.sh",
    "test-clean": "rm -rf ./coverage && yarn test-once --clearCache",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "svgo": "svgo -f ./src/assets/icons --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}; svgo -f ./src/assets/images --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}",
    "mparticle": "yarn install && yarn upgrade mparticle_json_validation && node scripts/mparticle.js",
    "analyze-bundle": "yarn buildprod --statsJson=true && yarn webpack-bundle-analyzer dist/es/stats.json",
    "libphonenumber-metadata": "./scripts/generate-metadata/generate-metadata.sh",
    "storybook": "start-storybook -p 6006 -s ./src/stories/assets,./src",
    "storybook-beta": "./scripts/docknum/update.sh beta && start-storybook",
    "i18n-helper": "ts-node scripts/i18n/i18n-helper.ts",
    "i18n-helper:merge-phrase": "ts-node scripts/i18n/i18n-helper.ts merge-phrase"
  },
  "private": true,
  "dependencies": {
    "@angular-slider/ngx-slider": "^2.0.3",
    "@angular/animations": "^12.2.12",
    "@angular/common": "^12.2.12",
    "@angular/compiler": "^12.2.12",
    "@angular/core": "^12.2.12",
    "@angular/forms": "^12.2.12",
    "@angular/localize": "~12.2.12",
    "@angular/platform-browser": "^12.2.12",
    "@angular/platform-browser-dynamic": "^12.2.12",
    "@angular/router": "^12.2.12",
    "@angular/service-worker": "^12.2.12",
    "@mparticle/web-appboy-kit": "^2.0.6",
    "@mparticle/web-google-analytics-kit": "^2.1.12",
    "@mparticle/web-sdk": "^2.12.3",
    "@ng-bootstrap/ng-bootstrap": "6.0.3",
    "@sentry/angular": "^6.8.0",
    "@sentry/tracing": "^6.15.0",
    "@types/faker": "^5.1.5",
    "bootstrap": "4.0.0",
    "crypto-js": "^3.1.9-1",
    "echarts": "^4.7.0",
    "faker": "^5.1.0",
    "hammerjs": "^2.0.8",
    "libphonenumber-js": "1.6.8",
    "lodash-es": "^4.17.15",
    "lottie-web": "^5.7.13",
    "moment": "^2.26.0",
    "ng2-dnd": "^5.0.2",
    "ngx-autosize": "1.6.4",
    "ngx-cookie": "4.0.2",
    "ngx-device-detector": "1.3.11",
    "ngx-echarts": "^4.2.2",
    "ngx-permissions": "^7.0.3",
    "ngx-pinch-zoom": "^2.5.5",
    "rxjs": "^6.6.3",
    "spain-id": "^1.0.9",
    "stanza.io": "https://github.com/Wallapop/stanza.io#v8.0.5",
    "tslib": "^2.0.0",
    "uuid": "^8.3.1",
    "visibilityjs": "^2.0.1",
    "web-animations-js": "^2.3.2",
    "web-styleguide": "git+ssh://git@github.com:Wallapop/web-styleguide#^0.7.1",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.12",
    "@angular-eslint/builder": "^12.0.0",
    "@angular-eslint/eslint-plugin": "^12.0.0",
    "@angular-eslint/eslint-plugin-template": "^12.0.0",
    "@angular-eslint/schematics": "12.6.1",
    "@angular-eslint/template-parser": "^12.0.0",
    "@angular/cli": "12.2.12",
    "@angular/compiler-cli": "^12.2.12",
    "@angular/language-service": "^12.2.12",
    "@babel/core": "^7.12.10",
    "@storybook/addon-essentials": "^6.2.7",
    "@storybook/angular": "^6.2.7",
    "@storybook/builder-webpack5": "^6.3.12",
    "@storybook/manager-webpack5": "^6.3.12",
    "@types/doubleclick-gpt": "^2019041801.0.4",
    "@types/echarts": "^4.6.0",
    "@types/heremaps": "3.1.5",
    "@types/jest": "^25.2.1",
    "@types/node": "^12.11.1",
    "@types/stripe-v3": "^3.1.21",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "babel-loader": "^8.2.2",
    "canvas": "^2.6.1",
    "eslint": "^7.26.0",
    "eslint-config-prettier": "^8.3.0",
    "glob": "^7.1.4",
    "husky": "^5.0.4",
    "jest": "^25.5.4",
    "jest-preset-angular": "^8.1.3",
    "json-schema-to-typescript": "^7.0.0",
    "mparticle_json_validation": "git+ssh://git@github.com:Wallapop/mparticle_json_validation#0.117.0",
    "prettier": "^2.2",
    "pretty-quick": "^3.1.0",
    "retry": "^0.12.0",
    "ts-node": "~7.0.0",
    "typescript": "4.3.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "xml2json": "^0.12.0"
  }
}
