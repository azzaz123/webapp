{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "builddev": "ng build --env=dev --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "builddocker": "ng build --env=docker --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "builddocker-en": "ng build --env=docker --aot",
    "buildbeta": "ng build --env=beta --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "buildbeta-en": "ng build --env=beta --aot",
    "buildprod": "ng build --prod --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "buildprod-en": "ng build --prod --aot",
    "i18n": "ng-xi18n -p src/tsconfig.app.json --i18nFormat=xmb",
    "serve": "ng serve --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "test": "ng test --watch=false  --no-progress",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.2.3",
    "@angular/common": "4.2.3",
    "@angular/compiler": "4.2.3",
    "@angular/core": "4.2.3",
    "@angular/forms": "4.2.3",
    "@angular/http": "4.2.3",
    "@angular/material": "^2.0.0-beta.3",
    "@angular/platform-browser": "4.2.3",
    "@angular/platform-browser-dynamic": "4.2.3",
    "@angular/router": "4.2.3",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-alpha.23",
    "@types/hammerjs": "^2.0.33",
    "@types/heremaps": "^3.0.10",
    "angular2-infinite-scroll": "^0.2.8",
    "angular2-masonry": "^0.4.0",
    "angular2-moment": "https://github.com/Wallapop/angular2-moment#2.2.0",
    "appboy-web-sdk": "2.0.1",
    "bootstrap": "4.0.0-alpha.6",
    "core-js": "^2.4.1",
    "lodash": "^4.17.4",
    "ng-elastic": "^1.0.0-beta.3",
    "ng-select": "^1.0.0-beta.6",
    "ng2-dnd": "^4.0.2",
    "ngx-cookie": "^1.0.0",
    "ngx-toastr": "^5.0.7",
    "ngx-uploader": "https://github.com/Wallapop/ngx-uploader#3.2.0",
    "rxjs": "5.3.0",
    "shield": "git+ssh://git@github.com/Wallapop/shield.git#1.20.5",
    "zone.js": "^0.8.4"
  },
  "devDependencies": {
    "@angular/cli": "1.1.1",
    "@angular/compiler-cli": "^4.0.0",
    "@angular/language-service": "^4.0.0",
    "@types/jasmine": "2.5.45",
    "@types/node": "~6.0.60",
    "codelyzer": "~3.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~3.0.4",
    "tslint": "~5.3.2",
    "typescript": "~2.3.3"
  }
