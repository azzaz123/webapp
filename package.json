{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "builddev": "ng build --env=dev --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker": "ng build --env=docker --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker-en": "ng build --env=docker --aot --no-progress",
    "buildbeta": "ng build --env=beta --output-hashing all --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildbeta-en": "ng build --env=beta --output-hashing all --aot --no-progress",
    "buildprod": "ng build --prod --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildprod-en": "ng build --prod --aot --no-progress",
    "i18n": "ng xi18n -p src/tsconfig.app.json --i18nFormat=xmb --outputPath=src/locale",
    "serve": "ng serve --aot --locale es --i18n-file src/locale/es.xtb --i18n-format xtb",
    "test": "ng test --single-run --no-progress --reporters spec --browsers ChromeHeadless",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "5.1.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/material": "5.1.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/platform-server": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@ng-bootstrap/ng-bootstrap": "1.0.0",
    "@types/hammerjs": "^2.0.33",
    "@types/heremaps": "^3.0.10",
    "angular2-masonry": "^0.4.0",
    "angular2-moment": "https://github.com/Wallapop/angular2-moment#2.2.0",
    "angular2-uuid": "^1.1.1",
    "appboy-web-sdk": "^2.0.7",
    "bootstrap": "4.0.0-alpha.6",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "crypto-js": "^3.1.9-1",
    "lodash": "^4.17.4",
    "ng-elastic": "^1.0.0-beta.3",
    "ng2-haversine": "^0.1.1",
    "ng-push": "0.2.1",
    "ng-select": "1.0.0-rc.3",
    "ng2-dnd": "^4.0.2",
    "ngx-cookie": "2.0.1",
    "ngx-infinite-scroll": "^0.8.3",
    "ngx-toastr": "8.1.1",
    "ngx-uploader": "https://github.com/Wallapop/ngx-uploader#3.2.1",
    "pouchdb": "6.1.0",
    "rxjs": "^5.5.6",
    "stanza.io": "https://github.com/Wallapop/stanza.io#v8.0.3",
    "visibilityjs": "^1.2.4",
    "web-animations-js": "^2.3.1",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-spec-reporter": "^0.0.32",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "2.6.2"
  }
}
