{
  "name": "webapp",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "./scripts/docknum/start.sh",
    "start-beta": "ng serve -c=beta",
    "builddev": "ng build --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker": "ng build --configuration=docker --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "builddocker-en": "ng build --configuration=docker --aot --no-progress",
    "buildbeta": "ng build --prod --configuration=beta --output-hashing all --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildbeta-en": "ng build --configuration=beta --output-hashing all --aot --no-progress",
    "buildprod": "ng build --prod --aot --i18n-locale es --i18n-file src/locale/es.xtb --i18n-format xtb --no-progress",
    "buildprod-en": "ng build --prod --aot --no-progress",
    "i18n": "ng xi18n --i18n-format=xmb --output-path=locale",
    "serve": "ng serve --aot --configuration=es",
    "test": "ng test --watch=false --no-progress --browsers HeadlessChrome",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "svgo": "svgo -f ./src/assets/icons --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}; svgo -f ./src/assets/images --disable={removeViewBox,cleanupIDs,removeUselessStrokeAndFill}",
    "mparticle": "node scripts/mparticle.js",
    "analyze-bundle": "yarn buildprod --statsJson=true && yarn webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@amcharts/amcharts3-angular": "^2.1.1",
    "@angular/animations": "^7.2.16",
    "@angular/cdk": "6.4.1",
    "@angular/common": "^7.2.16",
    "@angular/compiler": "^7.2.16",
    "@angular/core": "^7.2.16",
    "@angular/forms": "^7.2.16",
    "@angular/material": "6.4.1",
    "@angular/platform-browser": "^7.2.16",
    "@angular/platform-browser-dynamic": "^7.2.16",
    "@angular/router": "^7.2.16",
    "@mparticle/web-appboy-kit": "^2.0.4",
    "@mparticle/web-sdk": "^2.9.9-rc.1",
    "@ng-bootstrap/ng-bootstrap": "4.2.2",
    "@ngu/carousel": "^1.5.5",
    "@types/hammerjs": "^2.0.35",
    "@types/heremaps": "3.0.12",
    "angular2-moment": "https://github.com/Wallapop/angular2-moment#2.2.0",
    "angular2-uuid": "^1.1.1",
    "appboy-web-sdk": "^2.4.3",
    "bootstrap": "4.0.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.7",
    "crypto-js": "^3.1.9-1",
    "fingerprintjs2": "2.1.0",
    "indexeddb-getall-shim": "^1.3.5",
    "json-schema-to-typescript": "^7.0.0",
    "libphonenumber-js": "^1.6.8",
    "lodash-es": "^4.17.15",
    "ng-push": "0.2.2",
    "ng-select": "1.0.0",
    "ng2-dnd": "^5.0.2",
    "ng2-haversine": "^0.1.1",
    "ngx-autosize": "1.6.4",
    "ngx-cookie": "4.0.2",
    "ngx-device-detector": "1.3.11",
    "ngx-infinite-scroll": "^6.0.1",
    "ngx-permissions": "^5.0.0",
    "ngx-toastr": "10.2.0",
    "rxjs": "^6.5.4",
    "shelving-mock-indexeddb": "^1.1.0",
    "stanza.io": "https://github.com/Wallapop/stanza.io#v8.0.4",
    "tslib": "^1.9.0",
    "visibilityjs": "^2.0.1",
    "web-animations-js": "^2.3.1",
    "web-styleguide": "git+ssh://git@github.com:Wallapop/web-styleguide#^0.7.1",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "^7.2.16",
    "@angular/language-service": "^7.2.16",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~10.5.3",
    "codelyzer": "^4.4.2",
    "glob": "^7.1.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-spec-reporter": "^0.0.32",
    "mparticle_json_validation": "git+ssh://git@github.com:Wallapop/mparticle_json_validation#0.12.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.8.0",
    "retry": "^0.12.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "3.2.4",
    "webpack-bundle-analyzer": "^3.6.0"
  }
}
